# API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Database Configuration
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password

CHROMA_HOST=localhost
CHROMA_PORT=8000

# Model Profile Configuration
# Options: premium, balanced, cost_effective, gemini_only, openai_only
# Test modes: gemini_only (only Google API), openai_only (only OpenAI API)
MODEL_PROFILE=premium

# Legacy Model Configuration (Optional - overrides MODEL_PROFILE if all are set)
# CLASSIFIER_MODEL=gemini-2.5-flash
# EXTRACTOR_MODEL=gpt-4.1
# SYNTHESIZER_MODEL=claude-opus-4-20250514
# VALIDATOR_MODEL_1=gpt-4o
# VALIDATOR_MODEL_2=claude-sonnet-4-20250514

# Processing Settings
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
MAX_RETRIES=3

# LiteLLM Proxy Configuration
LITELLM_PROXY_URL=http://localhost:4000
# For Docker: LITELLM_PROXY_URL=http://litellm-proxy:4000

# ===== LITELLM PRODUCTION SECURITY =====
# LiteLLM UI Authentication
UI_PASSWORD=litellm-enterprise-admin-2025-CHANGE-ME

# LiteLLM Security Keys (CHANGE IN PRODUCTION!)
LITELLM_MASTER_KEY=sk-ki-system-master-2025-CHANGE-ME
LITELLM_SALT_KEY=sk-salt-ki-system-secure-2025-CHANGE-ME

# Database URLs
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/litellm_db 